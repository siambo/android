<?xml version="1.0" encoding="UTF-8"?>
<project name="WEAZRAPI_V1" default="dist" basedir=".">
    <description>Weazr webservice api</description>
    <property name="src" location="src" />
    <property name="bin" location="bin" />
    
    <target name="dist">
        <jar destfile="weazrapi.jar" basedir="bin/classes">
            <include name="com/**" />
        </jar>
        <copy file="weazrapi.jar" tofile="../Weazr/libs/weazrapi.jar" />
    </target>
</project>
